<h1>All Events</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= link_to 'Show', event %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<br>

<%= link_to 'New Event', new_event_path %>

<%= render 'events', prev_events: @past, upcoming_events: @upcoming %>
